{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Bitcore Blog",
    "owner": "Bitcore Team",
    "description": "A pure, powerful blog for your daily reading."
  },
  "baseUrl": "/blog/",
  "plugins": [
    "./plugins/paginator.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 20
  }
}
